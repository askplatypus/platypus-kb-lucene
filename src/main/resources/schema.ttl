@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix schema: <http://schema.org/> .


<http://kb.askplatyp.us/api/v1/schema> a owl:Ontology ;
    rdfs:label "Platypus knowledge base ontology" ;
    rdfs:comment "Ontology used by Platypus knowledge base. It is a restricted version of schema.org." .


# classes

schema:Airline a rdfs:Class ;
    rdfs:label "Airline" ;
    rdfs:comment "An airline company" ;
    rdfs:subClassOf schema:Organization .

schema:Airport a rdfs:Class ;
    rdfs:label "Airport" ;
    rdfs:comment "An airport" ;
    rdfs:subClassOf schema:Place .

schema:Book a rdfs:Class ;
    rdfs:label "Book" ;
    rdfs:comment "A book" ;
    rdfs:subClassOf schema:CreativeWork .

schema:City a rdfs:Class ;
    rdfs:label "City" ;
    rdfs:comment "A city or town" ;
    rdfs:subClassOf schema:Place .

schema:Country a rdfs:Class ;
    rdfs:label "Country" ;
    rdfs:comment "A country (past or present)" ;
    rdfs:subClassOf schema:Place .

schema:CreativeWork a rdfs:Class ;
    rdfs:label "CreativeWork" ;
    rdfs:comment "A work" .

schema:Organization a rdfs:Class ;
    rdfs:label "Organization" ;
    rdfs:comment "An organization" .

schema:Periodical a rdfs:Class ;
    rdfs:label "Periodical" ;
    rdfs:comment "A publication issued in successive parts" ;
    rdfs:subClassOf schema:CreativeWork .

schema:Person a rdfs:Class ;
    rdfs:label "Person" ;
    rdfs:comment "A person (real or fictional)" .

schema:Place a rdfs:Class ;
    rdfs:label "Place" ;
    rdfs:comment "Entities that have a somewhat fixed, physical extension" .


# object properties

schema:author a owl:ObjectProperty ;
    rdfs:label "author" ;
    rdfs:comment "Agent that has created this work" ;
    rdfs:domain schema:CreativeWork ;
    rdfs:range [ a rdfs:Class ; owl:unionOf ( schema:Person schema:Organization ) ] .

schema:birthPlace a owl:ObjectProperty, owl:FunctionalProperty ;
    rdfs:label "birthPlace" ;
    rdfs:comment "Birth place of the person" ;
    rdfs:domain schema:Person ;
    rdfs:range schema:Place .

schema:character a owl:ObjectProperty ;
    rdfs:label "character" ;
    rdfs:comment "Fictional person connected with a creative work" ;
    rdfs:domain schema:CreativeWork ;
    rdfs:range schema:Person .

schema:children a owl:ObjectProperty ;
    rdfs:label "children" ;
    rdfs:comment "Children of the person" ;
    rdfs:domain schema:Person ;
    rdfs:range schema:Person .

schema:contentLocation a owl:ObjectProperty ;
    rdfs:label "contentLocation" ;
    rdfs:comment "The location depicted or described in the content" ;
    rdfs:domain schema:CreativeWork ;
    rdfs:range schema:Place .

schema:contributor a owl:ObjectProperty ;
    rdfs:label "contributor" ;
    rdfs:comment "A secondary contributor to the CreativeWork" ;
    rdfs:domain schema:CreativeWork ;
    rdfs:range [ a rdfs:Class ; owl:unionOf ( schema:Person schema:Organization ) ] .

schema:creator a owl:ObjectProperty ;
    rdfs:label "creator" ;
    rdfs:comment "The creator/author of this CreativeWork" ;
    rdfs:domain schema:CreativeWork ;
    rdfs:range [ a rdfs:Class ; owl:unionOf ( schema:Person schema:Organization ) ] .

schema:deathPlace a owl:ObjectProperty, owl:FunctionalProperty ;
    rdfs:label "deathPlace" ;
    rdfs:comment "Death place of the person" ;
    rdfs:domain schema:Person ;
    rdfs:range schema:Place .

schema:editor a owl:ObjectProperty ;
    rdfs:label "editor" ;
    rdfs:comment "Specifies the Person who edited the CreativeWork" ;
    rdfs:domain schema:CreativeWork ;
    rdfs:range schema:Person .

schema:genre a owl:ObjectProperty ;
    rdfs:label "genre" ;
    rdfs:comment "Genre of the creative work" ;
    rdfs:domain schema:CreativeWork ;
    rdfs:range owl:Thing .

schema:geo a owl:ObjectProperty, owl:FunctionalProperty ;
    rdfs:label "geo" ;
    rdfs:comment "The geographic localisation of a place" ;
    rdfs:domain schema:Place ;
    rdfs:range [ a rdfs:Class ; owl:unionOf ( schema:GeoCoordinates schema:GeoShape ) ] .

schema:hasPart a owl:ObjectProperty ;
    rdfs:label "hasPart" ;
    rdfs:comment "Indicates a CreativeWork that is (in some sense) a part of this CreativeWork" ;
    rdfs:domain schema:CreativeWork ;
    rdfs:range schema:CreativeWork ;
    owl:inverseOf schema:isPartOf .

schema:illustrator a owl:ObjectProperty ;
    rdfs:label "illustrator" ;
    rdfs:comment "Agent that have illustrated this work" ;
    rdfs:domain schema:Book ;
    rdfs:range [ a rdfs:Class ; owl:unionOf ( schema:Person schema:Organization ) ] .

schema:isPartOf a owl:ObjectProperty ;
    rdfs:label "isPartOf" ;
    rdfs:comment "Indicates a CreativeWork that this CreativeWork is (in some sense) part of" ;
    rdfs:domain schema:CreativeWork ;
    rdfs:range schema:CreativeWork ;
    owl:inverseOf schema:hasPart .

schema:nationality a owl:ObjectProperty ;
    rdfs:label "nationality" ;
    rdfs:comment "Nationality of the person" ;
    rdfs:domain schema:Person ;
    rdfs:range schema:Place .

schema:parent a owl:ObjectProperty ;
    rdfs:label "parent" ;
    rdfs:comment "Parents of the person" ;
    rdfs:domain schema:Person ;
    rdfs:range schema:Person .

schema:publisher a owl:ObjectProperty ;
    rdfs:label "publisher" ;
    rdfs:comment "The publisher of the creative work" ;
    rdfs:domain schema:CreativeWork ;
    rdfs:range [ a rdfs:Class ; owl:unionOf ( schema:Person schema:Organization ) ] .

schema:translator a owl:ObjectProperty ;
    rdfs:label "translator" ;
    rdfs:comment "Agent that have translated this work" ;
    rdfs:domain schema:CreativeWork ;
    rdfs:range [ a rdfs:Class ; owl:unionOf ( schema:Person schema:Organization ) ] .

# datatype properties

schema:alternateName a owl:DatatypeProperty ;
    rdfs:label "alternateName" ;
    rdfs:comment "Alternative names for the entity" ;
    rdfs:domain owl:Thing ;
    rdfs:range rdf:langString .

schema:birthDate a owl:DatatypeProperty, owl:FunctionalProperty ;
    rdfs:label "birthDate" ;
    rdfs:comment "Birth date of the person" ;
    rdfs:domain schema:Person ;
    rdfs:range [ a rdfs:Datatype ; owl:unionOf ( xsd:dateTime xsd:date xsd:gYearMonth xsd:gYear ) ] .

schema:dateCreated a owl:DatatypeProperty, owl:FunctionalProperty ;
    rdfs:label "dateCreated" ;
    rdfs:comment "The date on which the CreativeWork was created" ;
    rdfs:domain schema:CreativeWork ;
    rdfs:range [ a rdfs:Datatype ; owl:unionOf ( xsd:dateTime xsd:date xsd:gYearMonth xsd:gYear ) ] .

schema:datePublished a owl:DatatypeProperty, owl:FunctionalProperty ;
    rdfs:label "datePublished" ;
    rdfs:comment "Date of first broadcast/publication" ;
    rdfs:domain schema:CreativeWork ;
    rdfs:range [ a rdfs:Datatype ; owl:unionOf ( xsd:dateTime xsd:date xsd:gYearMonth xsd:gYear ) ] .

schema:deathDate a owl:DatatypeProperty, owl:FunctionalProperty ;
    rdfs:label "deathDate" ;
    rdfs:comment "Death date of the person" ;
    rdfs:domain schema:Person ;
    rdfs:range [ a rdfs:Datatype ; owl:unionOf ( xsd:dateTime xsd:date xsd:gYearMonth xsd:gYear ) ] .

schema:description a owl:DatatypeProperty, owl:FunctionalProperty ;
    rdfs:label "description" ;
    rdfs:comment "A description of the entity" ;
    rdfs:domain owl:Thing ;
    rdfs:range rdf:langString .

schema:iataCode a owl:DatatypeProperty, owl:FunctionalProperty ;
    rdfs:label "iataCode" ;
    rdfs:comment "IATA code of a company, an airport or a city" ;
    rdfs:domain [ a rdfs:Class ; owl:unionOf ( schema:Airline schema:Airport schema:City ) ] ;
    rdfs:range xsd:string .

schema:icaoCode a owl:DatatypeProperty, owl:FunctionalProperty ;
    rdfs:label "icaoCode" ;
    rdfs:comment "ICAO code of an airport of a company" ;
    rdfs:domain [ a rdfs:Class ; owl:unionOf ( schema:Airline schema:Airport ) ] ;
    rdfs:range xsd:string .

schema:isbn a owl:DatatypeProperty ;
    rdfs:label "isbn" ;
    rdfs:comment "The ISBN of a book" ;
    rdfs:domain schema:Book ;
    rdfs:range xsd:string .

schema:issn a owl:DatatypeProperty ;
    rdfs:label "issn" ;
    rdfs:comment "The International Standard Serial Number (ISSN) that identifies this periodical" ;
    rdfs:domain schema:Periodical ;
    rdfs:range xsd:string .

schema:name a owl:DatatypeProperty, owl:FunctionalProperty ;
    rdfs:label "name" ;
    rdfs:comment "The entity name" ;
    rdfs:domain owl:Thing ;
    rdfs:range rdf:langString .

schema:sameAs a owl:DatatypeProperty ;
    rdfs:label "sameAs" ;
    rdfs:comment "URL of the official website of the entity" ;
    rdfs:domain owl:Thing ;
    rdfs:range xsd:anyURI .

schema:url a owl:DatatypeProperty, owl:FunctionalProperty ;
    rdfs:label "url" ;
    rdfs:comment "URL of the official website of the entity" ;
    rdfs:domain owl:Thing ;
    rdfs:range xsd:anyURI .
